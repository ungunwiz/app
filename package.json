{
  "name": "ungunwiz",
  "version": "0.0.1",
  "author": "Frederik 'zlyfer' Shull",
  "homepage": "https://ungunwiz.net/",
  "description": "A PUBG Weapon Stats App",
  "scripts": {
    "start": "npm run preBuild && ionic serve --no-open --configuration=development",
    "run-android": "ionic serve --no-open --configuration=app",
    "run-android_": "ionic cap run android -l --external --configuration=app",
    "run-ios": "ionic cap run ios -l --external --configuration=app",
    "run-freddy": "ionic cap run android -l --external --public-host=192.168.178.25 --target=2748e267 --configuration=app",
    "build-website": "npm run preBuild && npx ionic build --configuration=production && npx cap copy",
    "build-android": "npm run preBuild && npx ionic build --configuration=app.production && npx cap copy",
    "build-android_": "ionic build --configuration=app.production && ionic cap build android",
    "build-ios": "ionic build --configuration=app.production && ionic cap build ios",
    "generate-assets": "npx @capacitor/assets generate --android",
    "generate-assets_": "npx cordova-res --skip-config --copy",
    "buildWithTag": "node buildWithTag.js",
    "preBuild": "node preBuild.js",
    "eslint": "npx eslint src/app",
    "eslint-quiet": "npx eslint --quiet src/app",
    "eslint-fix": "npx eslint --fix src/app",
    "eslint-quiet-fix": "npx eslint --quiet --fix src/app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/router": "^17.0.2",
    "@angular/service-worker": "^17.0.2",
    "@capacitor-community/file-opener": "^1.0.6",
    "@capacitor/android": "5.5.1",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.5.1",
    "@capacitor/filesystem": "^5.2.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "5.5.1",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@ionic/angular": "^7.0.0",
    "@zlyfer/cap-plugin-install-permission": "^0.0.2",
    "chart.js": "^4.4.2",
    "ionicons": "^7.0.0",
    "ng2-charts": "^5.0.3",
    "rxjs": "~7.8.0",
    "swiper": "^11.0.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.1",
    "@angular-eslint/builder": "^17.0.0",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.0.0",
    "@angular-eslint/template-parser": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.2",
    "@angular/language-service": "^17.0.2",
    "@capacitor/cli": "5.5.1",
    "@ionic/angular-toolkit": "11.0.1",
    "@ionic/cli": "^7.2.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^8.3.0",
    "typescript": "~5.2.2"
